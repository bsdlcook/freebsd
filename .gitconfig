[commit]
	gpgsign = true
	template = ~/.gitmessage
[credential]
	helper = store --file ~/.git-credentials
[gpg]
	program = gpg2
[core]
	pager = cat
[diff]
	noprefix = true
[alias]
	cl = clone
	df = diff
	ll = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	st = status
        br = branch
        ci = commit
        co = checkout
        pu = push
