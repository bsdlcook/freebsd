source "$HOME/.config/common"

export GITLABDEVEL="$DEVELDIR/gitlab"
export GITLABUSER="lcook"
export GITLABDIR="$GITLABDEVEL/$GITLABUSER"
export GITLABURL="https://gitlab.com"
export GITLABAPI="$GITLABURL/api/v4/users"
export GITLABPROJECTS="$GITLABAPI/$GITLABUSER/projects"
export GITLABREPO="$GITLABURL/$GITLABUSER"
